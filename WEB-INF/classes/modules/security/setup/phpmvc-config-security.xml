<!-- Module Security -->
	<action-mappings module="Security">

		<action    path = "securityNoPermission"
							 type = "SecurityNoPermissionAction"
							 name = "securityNoPermission"
							scope = "request"
					 validate = "true">
			<forward name="success"  path="SecurityNoPermission.tpl" />
		</action>

		<action    path = "securityUsersEdit"
							 type = "SecurityUsersEditAction"
							 name = "securityUsersEdit"
							scope = "request"
					 validate = "true">
			<forward name="success"  path="SecurityUsersEdit.tpl" />
		</action>

		<action    path = "securityUsersDoEdit"
							 type = "SecurityUsersDoEditAction"
							 name = "securityUsersDoEdit"
							scope = "request"
					 validate = "true">
			<forward name="success" path="/Main.php?do=securityUsersEdit&amp;message=saved" redirect='true'/>
		</action>

		<action    path = "securityAffiliatesEdit"
							 type = "SecurityAffiliatesEditAction"
							 name = "securityAffiliatesEdit"
							scope = "request"
					 validate = "true">
			<forward name="success"  path="SecurityAffiliatesEdit.tpl" />
		</action>

		<action    path = "securityAffiliatesDoEdit"
							 type = "SecurityAffiliatesDoEditAction"
							 name = "securityAffiliatesDoEdit"
							scope = "request"
					 validate = "true">
			<forward name="success" path="/Main.php?do=securityAffiliatesEdit&amp;message=saved" redirect='true'/>
		</action>

		<action    path = "securityRegistrationEdit"
							 type = "SecurityRegistrationEditAction"
							 name = "securityRegistrationEdit"
							scope = "request"
					 validate = "true">
			<forward name="success"  path="SecurityRegistrationEdit.tpl" />
		</action>

		<action    path = "securityRegistrationDoEdit"
							 type = "SecurityRegistrationDoEditAction"
							 name = "securityRegistrationDoEdit"
							scope = "request"
					 validate = "true">
			<forward name="success" path="/Main.php?do=securityRegistrationEdit&amp;message=saved" redirect='true'/>
		</action>

	</action-mappings>

<!-- End Module Security -->

