<!-- Module Actionlogs -->
	<action-mappings module = "Actionlogs">

		<action    path = "actionlogsList"
					     type = "ActionlogsListAction"
					     name = "actionlogsList"
					    scope = "request"
					 validate = "true">
			<forward name="success" path="ActionlogsList.tpl"/>
		</action>

		<action    path = "actionlogsPurge"
					     type = "ActionlogsPurgeAction"
					     name = "actionlogsPurge"
					    scope = "request"
					 validate = "true">
			<forward name="success" path="ActionlogsPurge.tpl"/>
		</action>

		<action    path = "actionlogsDoPurge"
					     type = "ActionlogsDoPurgeAction"
					     name = "actionlogsDoPurge"
					    scope = "request"
					 validate = "true">
			<forward name="success" path="/Main.php?do=actionlogsList&amp;message=purged" redirect="true" />
		</action>

	</action-mappings>
<!-- End Module Actionlogs -->
