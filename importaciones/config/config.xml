<?xml version="1.0" encoding="utf8"?>
<config>
	<system>
		<parameters>
			<debugMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</debugMode>
			<developmentMode>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</developmentMode>
			<mailCapture>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</mailCapture>
			<underMaintenance>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</underMaintenance>
			<maintenanceMessage>El sistema permanecerá en mantenimiento por 2 horas, por favor regrese mas tarde.</maintenanceMessage>
			<affiliateUserLoginUnified>
				<value>YES</value>
				<element_type>YES/NO</element_type>
			</affiliateUserLoginUnified>
			<applicationTimeZoneGMT>
				<value>-4</value>
				<element_type>TIMEZONE</element_type>
			</applicationTimeZoneGMT>
			<dateFormat>
				<value>d-m-Y</value>
				<element_type>Options</element_type>
				<element_options>
					<option_1>d-m-Y</option_1>
					<option_2>m-d-Y</option_2>
					<option_3>d-m-y</option_3>
					<option_4>m-d-y</option_4>
				</element_options>
			</dateFormat>
			<thousandsSeparator>.</thousandsSeparator>
			<decimalSeparator>,</decimalSeparator>
			<numberOfDecimals>2</numberOfDecimals>
			<siteName>Brisway International Limited</siteName>
			<siteDescription>Sistema de Administración de Exportaciones de Brisway International Limited</siteDescription>
			<siteShortName>anmagaexport</siteShortName>
			<webmasterMail>gflamerich@gmail.com</webmasterMail>
			<debugMail>gflamerich@gmail.com</debugMail>
			<fromEmail>gflamerich@gmail.com</fromEmail>
			<tablePrefix></tablePrefix>
			<news>Novedades: Bienvenido al sistema Anmaga. En este momento este sistema se encuentra en período de prueba</news>
			<affiliateNews>Novedades: distinguidos Clientes, Bienvenido al sistema Anmaga. En este momento este sistema se encuentra en período de prueba</affiliateNews>
		</parameters>
		<errorReporting>
			<value>E_ALL ^ E_NOTICE</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1></option_1>
				<option_2>E_ALL ^ E_NOTICE</option_2>
				<option_3>E_ALL</option_3>
				<option_4>E_STRICT</option_4>
			</element_options>
		</errorReporting>
		<rowsPerPage>15</rowsPerPage>
	</system>
	<mluse>
		<language>esp</language>
	</mluse>
	<users>
		<licenses>5</licenses>
	</users>
	<email>
		<mode>
			<value>SMTP</value>
			<element_type>Options</element_type>
			<element_options>
				<option_1>PHP</option_1>
				<option_2>SMTP</option_2>
			</element_options>
		</mode>
		<smtpOptions>
			<smtpHost>mail.anmagaexport.modulserver.com.ar</smtpHost>
			<smtpPort>587</smtpPort>
			<smtpUsername>anmagaex</smtpUsername>
			<smtpPassword>Anmaga</smtpPassword>
			<smtpSsl>
				<value>NO</value>
				<element_type>YES/NO</element_type>
			</smtpSsl>
		</smtpOptions>
	</email>
	<import>
