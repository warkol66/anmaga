<!-- Module Import-->

<!-- Modulo ImportProducts -->
  <action-mappings>
    <action    path = "importProductsEdit"
               type = "ImportProductsEditAction"
               name = "importProductsEdit"
              scope = "request"
           validate = "true">
    <forward name="success" path="ImportProductsEdit.tpl" />
    </action>

    <action    path = "importProductsDoEdit"
               type = "ImportProductsDoEditAction"
               name = "importProductsDoEdit"
              scope = "request"
           validate = "true">
    <forward name="success" path="/Main.php?do=importProductsList&amp;message=ok" redirect="true" />
    <forward name="failure" path="ImportProductsEdit.tpl" />
    </action>

    <action    path = "importProductsList"
               type = "ImportProductsListAction"
               name = "importProductsList"
              scope = "request"
           validate = "true">
    <forward name="success" path="ImportProductsList.tpl" />
    </action>

    <action    path = "importProductsDoDelete"
               type = "ImportProductsDoDeleteAction"
               name = "importProductsDoDelete"
              scope = "request"
           validate = "true">
    <forward name="success" path="/Main.php?do=importProductsList&amp;message=deleted_ok" redirect="true" />
    </action>
    
     <action    path = "importProductsDoActivate"
               type = "ImportProductsDoActivateAction"
               name = "importProductsDoActivate"
              scope = "request"
           validate = "true">
    <forward name="success" path="/Main.php?do=importProductsList&amp;message=activated_ok" redirect="true" />
    </action>



    
  </action-mappings>
<!-- Fin Modulo ImportProducts -->
