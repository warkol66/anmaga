<?xml version="1.0" encoding="ISO-8859-1"?>

<config>
 <log>
  <type>file</type>
  <name>propel.log</name>
  <ident>anmaga</ident>
  <level>7</level>
 </log>
 <propel>
  <datasources default="anmaga">
   <datasource id="anmaga">
    <!-- the Propel adapter (usually same as phptype of connection DSN) -->
    <adapter>mysql</adapter>
    <connection>
     <dsn>mysql:host=localhost; dbname=</dsn>
     <user></user>
     <password></password>
    </connection>
   </datasource>
   <datasource id="mluse">
    <!-- the Propel adapter (usually same as phptype of connection DSN) -->
    <adapter>mysql</adapter>
    <connection>
     <dsn>mysql:host=localhost; dbname=</dsn>
     <user></user>
     <password></password>
    </connection>
   </datasource>
  </datasources>
  <debugpdo>
		<logging>
			<details>
				<method>
					<enabled>true</enabled>
				</method>
				<time>
					<enabled>true</enabled>
				</time>
				<mem>
					<enabled>true</enabled>
				</mem>
			</details>
		</logging>
   </debugpdo>
 </propel>
</config>